ck=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14005493.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14005938.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14005938.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14010819.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14010819.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14012026.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14012026.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14013097.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14013097.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14014739.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14014739.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;br/&gt;&lt;img src=&quot;http://www.srimg.com/u/20180228/14020057.gif&quot; onclick=&quot;window.open('http://www.viidii.info/?action=image&amp;amp;url=http%3A%2F%2Fwww.srimg.com%2Fi%2F%3Fi%3Du%2F20180228%2F14020057.gif&amp;amp;src='+encodeURIComponent(this.src));&quot;/&gt; &lt;/p&gt;
